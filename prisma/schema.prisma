generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}


model User {
  id            String   @id @default(uuid())
  username      String   
  password      String    
  email          String
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
  @@index([id])
}
